*"><xsl:variable name="att_name" select="name()" /><xsl:variable name="att_value" select="." /><xsl:value-of select="DisplayComps:printValue('TCAPPrice_SD_', $att_name, $att_value )" /></xsl:for-each><xsl:value-of select="DisplayComps:doPrintValues($fullcolcount)" /></tr><tr><xsl:value-of select="DisplayComps:writeStringFullColumnTD(@Description, $fullcolcount)" /></tr></xsl:template></xsl:stylesheet>